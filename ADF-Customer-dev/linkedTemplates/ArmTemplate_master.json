{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Customer-dev"
		},
		"AzureBlobStorage_ls_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "stgeastus2dev-conn"
		},
		"AzureKeyVaultPartner_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://PartnerKeys.vault.azure.net/"
		},
		"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-dataops-dev.vault.azure.net/"
		},
		"AzureSqlDatabaseAppDb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "AppDb-Conn"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-Customer-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVaultPartner_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultPartner_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_ls_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorage_ls_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorage_ls_properties_typeProperties_connectionString_secretName')]"
					},
					"AzureSqlDatabaseAppDb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDatabaseAppDb_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}